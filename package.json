{
  "name": "a2-codepal-helper",
  "displayName": "CodePal AI Helper",
  "description": "CodePal.ai Unofficial Extension",
  "publisher": "andruhovski",
  "repository": {
    "url": "https://github.com/andruhovski/a2-codepal-helper",
    "type": "git"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "a2-codepal-helper.runCodeDoc",
        "title": "A2 CodePal: Code Documentation"
      },
      {
        "command": "a2-codepal-helper.setApiKey",
        "title": "A2 CodePal: Set API Key"
      },
      {
        "command": "a2-codepal-helper.delApiKey",
        "title": "A2 CodePal: Clear API Key"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.88.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  }
}
